{
  "rewrites": [
    {
      "source": "/news/:date-candidates.md",
      "destination": "https://raw.githubusercontent.com/octmarker/ai-news-bot/main/news/:date-candidates.md"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type"
        }
      ]
    }
  ]
}
